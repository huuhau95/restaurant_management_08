<section id="book-a-table-pro">
  <div class="container">
    <h1>
      <%= t "users.login" %>
    </h1>
  </div>
  <div class="container">
    <div class="row">
      <%= form_for :session, url: login_path do |f| %>
        <div class="fix-background-book-table item-input-pro-footer">
          <div class="col-md-2"></div>
          <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
            <div class="fix-background-book-table item-input-pro-footer">
              <div class="col-xs-2 col-sm-3 col-md-3 col-lg-2">
                <%= image_tag "icon21.png" %>
              </div>
              <div class="col-xs-10 col-sm-9 col-md-9 col-lg-10">
                <%= f.email_field :email, placeholder: t("users.email") %>
              </div>
            </div>
            <div class="boder-bottom-foter-pro2"></div>
          </div>
          <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
            <div class="fix-background-book-table item-input-pro-footer">
              <div class="col-xs-2 col-sm-3 col-md-3 col-lg-2">
                <%= image_tag "icon18.png" %>
              </div>
              <div class="col-xs-10 col-sm-9 col-md-9 col-lg-10">
                <%= f.password_field :password, placeholder: t("users.password") %>
              </div>
            </div>
            <div class="boder-bottom-foter-pro2"></div>
          </div>
          <div class="col-md-2"></div>
          <div class="col-md-offset-5 col-md-3">
            <%= f.label :remember_me, class: "checkbox inline" do %>
              <%= f.check_box :remember_me %>
              <span><%= t "users.remember" %></span>
            <% end %>
            <%= f.submit t("users.title_login"), class: "btn-login" %>
          </div>
        </div>
      <% end %>
  </div>
  </div>
</section>
